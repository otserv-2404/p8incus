function _update()
 keyprs=stat(31)
 
 if keyprs=="a" then
  if viewx-6>0 then
   viewx-=1
  end
 elseif keyprs=="d" then
  if viewx+6<127 then
   viewx+=1
  end
 elseif keyprs=="w" then
  if viewy-6>0 then
   viewy-=1
  end
 elseif keyprs=="s" then
  if viewy+6<127 then
   viewy+=1
  end
 end
 
 if btnp(ðŸ…¾ï¸) then
  grid=not grid
 end
 
 if btnp(âŽ) then
  auto_tile()
 end
 
 update_cursor()
end

function _draw()
 cls(1)
 
