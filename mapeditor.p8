--init functions
function init_variables()
 --init maps
 maps={}
 currflr=3
 
 for floor=1,6 do
  --add new floor table
  add(maps,{})
  
  --create x and y axis
  for i=1,128 do
   maps[floor][i]={}
   
   --fill with blanks
   for j=1,128 do
    --if underground, spr77
    tile=77
    --if above ground, spr93
    if floor==4 then
     tile=93
    elseif floor>4 then
     tile=95
    end
    maps[floor][i][j]=tile
   end
  end
 end
 
 
 --mapstring={}
 mapstring="x385.d8x9.76x117.d4x9.06x117.d54x8.06x118.d54x7.06x119.d4x7.06x119.d4x7.06x119.d4x7.06x119.d409x5.5cx119.dx2.5cx14969.d1"
