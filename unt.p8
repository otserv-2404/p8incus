function draw_spr_ordered()
 --copy the global obj list
 --into a local one
 --(drawing from a global table
 --makes the game super slow)
 local drwobjlist={}
 for i,v in ipairs(objlist) do
  drwobjlist[i]=objlist[i]
 end 
 --this code is here temporarily
 --add player
 local obj=p
 add(drwobjlist,obj)
 
 --add enemy
 obj={sp=espr,x=ex,y=ey,spx=esprx,spy=espry,bar=true}
 add(drwobjlist,obj)
 --set impassbl (character = 3)
 wrldmap[obj.x/8][obj.y/8]=3 
 
 --sort objs by their y coord
 qsort(drwobjlist,function(a,b) return a.y<b.y end)
 local prev=0
 local l,r=1,0
 for i,v in ipairs(drwobjlist) do
  --if the next elem is diff
  --from prev, a new y pos must
  --be sorted
  if v.y~=prev or i==#drwobjlist then
   if i==#drwobjlist then
    i+=1
   end
   prev=v.y
   r=i-1
   --except if it's the first
   if r>0 and l<=r then
    qsort(drwobjlist,function(a,b) return a.x<b.x end,l,r)
   end
   l=i
  end
 end
 
 
 for i=1,#drwobjlist do
  local drwobj=drwobjlist[i]
  --if its a character
  if drwobj.sp<80 then
   draw_outline(drwobj.sp,drwobj.spx-2,drwobj.spy-2,1)
  --if its scenery
  elseif drwobj.sp==80 then
   sspr((drwobj.sp%16)*8,(drwobj.sp/16)*8,16,10,drwobj.x-8,drwobj.y-2)
  elseif drwobj.sp==82 then
   sspr((drwobj.sp%16)*8,(flr(drwobj.sp/16))*8,10,16,drwobj.x-2,drwobj.y-8)
  --81 itself isnt a thing, but
  --here it's the reprs. of a
  --corner for the walls 80,82
  elseif drwobj.sp==81 then
   line(drwobj.x-2,drwobj.y-2,drwobj.x+7,drwobj.y+7,4)
   rectfill(drwobj.x-2,drwobj.y-2,drwobj.x-1,drwobj.y-1,4)
  --tree
  elseif drwobj.sp==83 then
   sspr(26,40,10,10,drwobj.x-2,drwobj.y-2)
  elseif drwobj.sp==84 then
   sspr(36,40,10,10,drwobj.x-2,drwobj.y-2)
  elseif drwobj.sp==86 then
   sspr(48,40,16,16,drwobj.x-8,drwobj.y-8)
  end
 end
end

