 --setup 2nd floor
 --set all tiles as nothing(-1)
 mapz1={}
 for i=1,mapsize do
  mapz1[i]={}
  for j=1,mapsize do
   mapz1[i][j]=-1
  end
 end
 
 --make a roof(spr 67)
 for i=1,4 do
  for j=1,4 do
   mapz1[25+i][17+j]=67
  end
 end
 
 --set fallthrough opening
 --(spr 126)
 mapz1[26][19]=126
 
 --is this a hack? set a
 --"flag floor" to look inside
 --(spr 127)
 mapz1[27][22]=127
 -------------------
 
 objlist={}
 
 movq={}
 msglist={}
 
 movsize=8
 fpt=5 // frames per tick
 
 --player variables
 -- global position
 p={x=27*8,
    y=20*8,
    
    --sprite position
    spx=27*8,
    spy=20*8,
    --sprite number
    sp=1,
    
    speed=4,
    
    lv=2,
    exp=100,
    
    hpm=155,
    hp=155,
    mpm=5,
    mp=5,
    
    ismov=false,
    movdir=nil,
    movt=nil}
 
 
 --enemy variables
 ex=144
 ey=128
 esprx=144
 espry=128
 espr=4
 espeed=4 --px per tick
 
 elv=1
 ehpmax=50
 ehp=10
 empmax=0
 emp=0
 
 --enemy is moving state
 eismov=false
 emovdir=nil
 emovt=nil
 
 init_map()
 
 -- debug
 moveto=64
 ispassbl=0
end

function _update()
 if t%fpt==0 then
  updatetick()
 end
 
 updateanim()
 
 // queue movement
 if btnp(⬅️) then
  qmove("left")
 end
 if btnp(➡️) then
  qmove("right")
 end
 if btnp(⬆️) then
  qmove("up")
 end
 if btnp(⬇️) then
  qmove("down")
 end
 
 if btnp(🅾️) then
  send_message(p.spx,p.spy,"hail pico-8!","player")
 end
 
 t+=1
end

