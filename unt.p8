function init_map()
 wrldmap={}
 
 --get map values for ground
 --tiles
 for i=1,mapsize do
  wrldmap[i]={}
  for j=1,mapsize do
   wrldmap[i][j] = fget(mget(i,j))
  end
 end
 
 --add walls
 for i=1,4 do
  --horizontal wall
  local obj={sp=80,x=8*(25+i),y=17*8}
  --set pos x,y on map as
  --impassable (terrain = 1)
  wrldmap[25+i][17]=1
  add(objlist,obj)
  
  if i~=2 then
   local obj={sp=80,x=8*(25+i),y=21*8}
   wrldmap[25+i][21]=1
   add(objlist,obj)
  end
 end
 
 
 for i=1,4 do
  --vertical wall
  local obj={sp=82,x=25*8,y=(17+i)*8}
  wrldmap[25][17+i]=1
  add(objlist,obj)
  local obj={sp=82,x=29*8,y=(17+i)*8}
  wrldmap[29][17+i]=1
  add(objlist,obj)
 end
 
 local obj={sp=81,x=25*8,y=17*8}
 wrldmap[25][17]=1
 add(objlist,obj)
 
 --add trees 
 local trees={{19,18},{12,16},{16,16},{15,21},{23,22},{19,26},{23,14},{34,16}}
 for i,v in ipairs(trees) do
  local vx,vy=trees[i][1],trees[i][2]
  local obj={sp=83,x=vx*8,y=vy*8}
  wrldmap[vx][vy]=1
  add(objlist,obj)
 end
 
 --add rocks
 local rocks={{22,19},{23,19},{23,18},{24,17}}
 for i,v in ipairs(rocks) do
  local vx,vy=rocks[i][1],rocks[i][2]
  local obj={sp=84,x=vx*8,y=vy*8}
  wrldmap[vx][vy]=1
  add(objlist,obj)
 end
 
 --add stairs
 local obj={sp=86,x=26*8,y=19*8}
 wrldmap[26][19]=4
 add(objlist,obj)
end
